<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author %>
  </p>

  <p>
    <strong>Publisher:</strong>
    <%= book.publisher %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= book.year %>
  </p>

  <p>
    <strong>Borrowed at:</strong>
    <%= book.borrowed_at %>
  </p>

  <p>
    <strong>Available:</strong>
    <%= book.available %>
  </p>

  <p>
    <strong>Subject:</strong>
    <%= book.subject_id %>
  </p>

</div>
